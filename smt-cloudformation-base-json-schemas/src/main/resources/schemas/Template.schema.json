{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "Template.schema.json",
  "title": "Template",
  "description": "https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/template-anatomy.html",
  "type": "object",
  "javaType": "aws.Template",
  "javaInterfaces": [
    "aws.HasParameters<Template, Parameter>",
    "aws.HasConditions<Template>",
    "aws.HasResources<Template, Resource>",
    "aws.HasOutputs<Template, Output>"
  ],
  "properties": {
    "AWSTemplateFormatVersion": {
      "type": "string",
      "default": "2010-09-09"
    },
    "Description": {
      "type": "string"
    },
    "Metadata": {
      "type": "object",
      "javaType": "java.util.Map<String, Object>"
    },
    "Parameters": {
      "type": "object",
      "javaType": "java.util.Map<String, Parameter>"
    },
    "Mappings": {
      "type": "object",
      "javaType": "java.util.Map<String, java.util.Map<String, java.util.Map<String, String>>>"
    },
    "Conditions": {
      "type": "object",
      "javaType": "java.util.Map<String, aws.fn.ConditionFunction>"
    },
    "Resources": {
      "type": "object",
      "javaType": "java.util.Map<String, Resource>"
    },
    "Outputs": {
      "type": "object",
      "javaType": "java.util.Map<String, Output>"
    }
  }
}